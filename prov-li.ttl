###  http://linkedmap.unizar.es/def/prov-li#hadScope
:hadScope rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf owl:topObjectProperty ;
    rdfs:domain :Scope ;
    rdfs:range prov:Plan ;
    rdfs:label "hadScope" ;
    rdfs:definition """
    This property relates a derivation to a plan that constraints 
    the usage of entities used in a derivation to those parts that 
    meet some characteristics (e.g. temporal, spatial, subject)."""@en ;
    rdfs:isDefinedBy <http://linkedmap.unizar.es/def/prov-li#> .

###  http://linkedmap.unizar.es/def/prov-li#primaryTopic
:primaryTopic rdf:type owl:FunctionalProperty , owl:ObjectProperty ;
    rdfs:subPropertyOf prov:wasDerivedFrom ;
    rdfs:domain prov:Bundle ;
    rdfs:range prov:Entity ;
    rdfs:label "primaryTopic" ;
    rdfs:definition """
    The functional primaryTopic property relates a bundle to the main
    entity that the bundle is about."""@en ;
    rdfs:isDefinedBy <http://linkedmap.unizar.es/def/prov-li#> ;
    rdfs:seeAlso foaf:primaryTopic .

###  http://linkedmap.unizar.es/def/prov-li#qualifiedScope
:qualifiedScope rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf prov:qualifiedDerivation ;
    rdfs:range :Scope ;
    rdfs:domain prov:Entity ;
    rdfs:label "qualifiedScope" ;
    rdfs:definition """
    A scope is a derivation where a plan that constraints the usage of 
    entities used in the derivation to those parts that meet some 
    characteristics (e.g. temporal, spatial, subject)."""@en ;
    rdfs:isDefinedBy <http://linkedmap.unizar.es/def/prov-li#> .
    
###  http://linkedmap.unizar.es/def/prov-li#wasWithinScopeOf
:wasWithinScopeOf rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf prov:wasDerivedFrom ;
    rdfs:domain prov:Entity ;
    rdfs:range prov:Plan ;
    rdfs:label "wasWithinScopeOf" ;
    rdfs:definition """
    A scope is a derivation where a plan that constraints the usage of 
    entities used in the derivation to those parts that meet some 
    characteristics (e.g. temporal, spatial, subject)."""@en ;
    rdfs:isDefinedBy <http://linkedmap.unizar.es/def/prov-li#> .
    
###  http://linkedmap.unizar.es/def/prov-li#Scope
:Scope rdf:type owl:Class ;
    rdfs:subClassOf prov:Derivation ;
    rdfs:label "Scope" ;
